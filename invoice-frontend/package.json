{
  "name": "invoice-app-project-root",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://localhost:5173/invoice-web-app/",
  "scripts": {
    "dev": "cross-env VITE_BACKEND_URL=https://localhost:8000 vite",
    "demo": "cross-env VITE_BACKEND_URL=http://localhost:8000 vite",
    "dev:container": "cross-env VITE_BACKEND_URL=https://backend-dev:8000/graphql vite",
    "coverage": "vitest run --coverage",
    "build:local": "cross-env VITE_BACKEND_URL=http://backend.localhost/api/graphql VITE_REDIRECT_URI=http://frontend.localhost vite build",
    "build": "cross-env VITE_BACKEND_URL=https://localhost:8000 vite build",
    "preview": "vite preview",
    "test:unit": "vitest tests",
    "test:e2e": "cross-env VITE_BACKEND_URL=https://localhost:8000/graphql NODE_ENV=test playwright test --config=playwright.config.ts",
    "test:container": "cross-env VITE_BACKEND_URL=https://backend-test:8000/graphql vite",
    "test:e2e:docker": "cd ../ && cross-env VITE_BACKEND_URL=https://backend-dev:8000/graphql docker compose -f docker-compose.dev.yml create playwright-e2e && cross-env VITE_BACKEND_URL=https://backend-dev:8000/graphql docker compose -f docker-compose.dev.yml start playwright-e2e",
    "lint": "eslint --config ./eslint.config.js .",
    "lint:fix": "eslint --config ./eslint.config.js --fix .",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "format": "prettier --config ./prettier.config.js --write '**/*.ts*'",
    "codegen": "cross-env VITE_BACKEND_URL=https://localhost:8000/graphql playwright codegen https://localhost:5173/invoice-web-app/ --ignore-https-errors"
  },
  "dependencies": {
    "@apollo/client": "^3.12.2",
    "@auth0/auth0-react": "^2.2.4",
    "@reduxjs/toolkit": "^2.6.1",
    "@shelf/react-outside-click": "^0.1.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.14.2",
    "motion": "^12.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-datepicker": "^7.6.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-loader-spinner": "^6.1.6",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.3.0",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.11",
    "uuid": "10.0.0",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@axe-core/playwright": "^4.10.1",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.51.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.0",
    "@types/react-datepicker": "^4.19.2",
    "@types/react-dom": "^19.0.0",
    "@types/react-redux": "^7.1.33",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.0.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "playwright": "^1.51.0",
    "prettier": "3.4.2",
    "prettier-eslint": "^16.4.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.17.0",
    "typescript-plugin-styled-components": "^3.0.0",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "3.0.5",
    "vitest-mock-extended": "^3.0.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  },
  "packageManager": "yarn@4.6.0"
}
